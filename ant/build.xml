<?xml version="1.0" encoding="UTF-8"?>
<project name="RunnerTest" default="test" basedir=".">
    <property name="reports" location="FinalReports" />
    <target name="test" >
        <junitreport todir="${reports}">
            <fileset dir="${reports}/">
                <include name="*.xml" />
            </fileset>
            <report format="noframes" todir="${reports}\" />
        </junitreport>
    </target>
</project>
